{
  "name": "@web42-ai/site-director",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "description": "Site Director Express server for web42-ai platform",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint . --max-warnings 0",
    "check-types": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "dependencies": {
    "express": "^4.19.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.15.3",
    "@web42-ai/eslint-config": "workspace:*",
    "@web42-ai/typescript-config": "workspace:*",
    "eslint": "^9.31.0",
    "tsx": "^4.19.2",
    "typescript": "5.8.2",
    "vitest": "^2.1.8"
  }
}